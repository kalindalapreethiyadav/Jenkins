pipeline {
    agent any
    environment{
        Env_url = "roboshop.robo.com"
    }

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('stage two') {
            steps {
              echo "I am learning Jenkins"
              echo "CI & CD"
              echo "For DevOPs"   
            }
        }

        stage('stage three') {
            environment{
        Env_url = "preethi.local.com"
    }
            steps {
               sh "echo Env_url : $Env_url"
            }
        }
    }
}
